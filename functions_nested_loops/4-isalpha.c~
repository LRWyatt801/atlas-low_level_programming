#include "main.h"

/**
 * _isalpha - tests input for uppercase letter,
 *            then outputs a 1 for true and 0 for false
 * @c: character to be tested
 *
 * Return: int
 */

int _isalpha(int c)
{
	if (c >= 65 && c <= 90)
		return (1);
	return (0);
}
